{
    "name": "laravel-validation-rules/ip",
    "description": "Validate if an ip address is public or private.",
    "type": "library",
    "keywords": [ "laravel", "validation", "ip" ],
    "require": {
        "illuminate/contracts": "^5.5|^6.0|^7.0|^8.0",
        "symfony/http-foundation": "^3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^5.0",
        "phpunit/phpunit": "^6.3|^9.0"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ashley Clarke",
            "email": "me@ashleyclarke.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "LVR\\IP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LVR\\IP\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
